		<%- include('../partials/header'); -%>

		<div class="wrapper">
			
			<h1>VELDU ÞÍNAR UPPÁHALDS FRÉTTAVEITUR & ÁHUGAMÁL</h1>
			<div class="news-wrapper">
				<% if (user.settings.fb) { %>
				<div class="news fb true">
					<img src="" alt="fretta mynd" />
